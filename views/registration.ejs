<%- include("header", {title: "Registration" })%>
<nav>
    <div class="pages">
        <button onclick="location.href='/playlists';">Playlists</button>
        <button onclick="location.href='/artists';">Artists</button>
    </div>
    <button onclick="location.href='/'">Login</button>
</nav>
<div class="login-form form">
    <form method="post">
        <input
                name="usr"
                id="usr"
                type="text"
                alt="username"
                placeholder="Username"
        />
        <input
                name="psswrd"
                id="psswrd"
                type="password"
                alt="password"
                placeholder="Password"
        />
        <button type="submit">Register</button>
    </form>
</div>
<%- include("footer")%>
